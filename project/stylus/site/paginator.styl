nav.pagination
	display block
	overflow hidden
	height 1%
	ul
		float right
		margin-top 10px
		margin-bottom 10px
		li
			display inline
			a
				z-index 2
				color #fff
				cursor pointer
				border-color #337ab7
				float left
				padding 6px 12px
				margin-left -1px
				color #337ab7
				text-decoration none
				background-color #fff
				border 1px solid #ddd
			&.active
				a
					background-color color
					color white